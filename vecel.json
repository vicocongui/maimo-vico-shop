{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": ".*",
        "methods": ["GET", "POST", "DELETE", "PUT", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
          "Access-Control-Allow-Credentials": "true"
        },
        "dest": "app.js",
        "continue": true
      },
      {
        "src": "/products/(?<id>[^/]+)",
        "dest": "/app.js/?id=$id",
        "methods": ["DELETE"]
      },
      {
        "src": "/products/(?<id>[^/]+)",
        "dest": "/app.js/?id=$id",
        "methods": ["PUT"]
      },
      {
        "src": "/products",
        "dest": "/app.js",
        "methods": ["GET"]
      },
      {
        "src": "/products/(?<id>[^/]+)",
        "dest": "/app.js",
        "methods": ["GET"]
      },
      {
        "src": "/products",
        "dest": "/app.js",
        "methods": ["POST"]
      },
      {
        "src": "/orders/(?<id>[^/]+)",
        "dest": "/app.js/?id=$id",
        "methods": ["DELETE"]
      },
      {
        "src": "/orders/(?<id>[^/]+)",
        "dest": "/app.js/?id=$id",
        "methods": ["PUT"]
      },
      {
        "src": "/orders",
        "dest": "/app.js",
        "methods": ["GET"]
      },
      {
        "src": "/orders/(?<id>[^/]+)",
        "dest": "/app.js",
        "methods": ["GET"]
      },
      {
        "src": "/orders",
        "dest": "/app.js",
        "methods": ["POST"]
      }
    ]
  }
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  